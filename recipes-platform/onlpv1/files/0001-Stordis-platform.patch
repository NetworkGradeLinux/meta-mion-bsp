+#! /usr/bin/env bash
+
+### BEGIN INIT INFO
+# Provides:        bf2556x-1t-config
+# Required-Start:  $syslog $network
+# Required-Stop:   $syslog
+# Default-Start:   2 3 4 5
+# Default-Stop:    0 1 6
+# X-Start-Before:	onlpd onlp-snmpd
+# Short-Description: bf2556x platform configuration
+# Description:        bf2556x platform configuration
+### END INIT INFO
+
+PATH=/sbin:/bin:/usr/sbin:/usr/bin
+
+. /lib/lsb/init-functions
+
+
+PLATFORM=x86-64-stordis-bf2556x-1t
+REVISION=r0
+KERNEL=$(uname -r)
+KERNEL_MAJOR_MINOR=$(uname -r | awk -F. '{print $1 "." $2}')
+LINK=/lib/modules/${KERNEL}/build
+
+NETMODE=$(grep NETAUTO /etc/onl/boot-config | cut -d= -f 2)
+
+
+function print_to_console () {
+	echo $@
+}
+
+
+function safe_link () {
+	local TARGET=$1
+	local LINK=$2
+
+	if [[ ! -e $TARGET ]]; then
+		print_to_console "Target ${TARGET} does not exist"
+		return
+	fi
+
+
+	if [[ -L $LINK ]]; then
+		# Link exists
+		CURRENT_LINK=$(readlink $LINK)
+		if [[ "$CURRENT_LINK" != "$TARGET" ]]; then
+			# Only re-link if target is not what we expected
+			print_to_console "Re-linking ${TARGET} -> ${LINK} (was $CURRENT_LINK)" 
+			OUTPUT=$(ln -sf ${TARGET} ${LINK})
+			print_to_console $OUTPUT
+		fi
+	else
+		# Create folder if doesn't exist
+		mkdir -p $(dirname $LINK)
+
+		print_to_console "Linking ${TARGET} -> ${LINK}" 
+		OUTPUT=$(ln -s ${TARGET} ${LINK})
+		print_to_console $OUTPUT
+	fi
+}
+
+
+function update_dns_from_lease () {
+	# Legacy function, not used currently.
+	DNS_SERVERS=`grep domain-name-servers /var/lib/dhcp/dhclient.leases | awk '{print $3}' | cut -d\; -f1`
+	print_to_console "DNS_SERVERS: $DNS_SERVERS"
+	if [[ $DNS_SERVERS != "" ]]; then
+		IFS=',' read -r -a DNS_SERVER_ARRAY <<< $DNS_SERVERS
+		print_to_console "${DNS_SERVER_ARRAY[@]}: $DNS_SERVERS"
+		for DNS_SERVER in ${DNS_SERVER_ARRAY[@]}; do
+			grep $DNS_SERVER /etc/resolvconf/resolv.conf.d/base
+			if [[ $? == 1 ]]; then
+				print_to_console "Preparing DNS ${DNS_SERVER}"
+				echo nameserver $DNS_SERVER >> /etc/resolvconf/resolv.conf.d/base
+			else 
+				print_to_console "${DNS_SERVER} is already in /etc/resolvconf/resolv.conf.d/base"
+			fi
+		 done
+		 resolvconf -u
+	fi
+}
+
+
+function setup_dns () {
+	if [[ "$NETMODE" != "dhcp" ]]; then
+		print_to_console "Network mode is not DHCP, not running dhclient"
+		return
+	fi
+
+	print_to_console "Running dhclient for DNS settings"
+	dhclient -r -i ma1
+	dhclient -i ma1
+}
+
+
+function disable_automount () {
+	PROC_AUTOMOUNT_STATUS=$(systemctl is-enabled proc-sys-fs-binfmt_misc.automount)
+	if [[ "${PROC_AUTOMOUNT_STATUS}" != "masked" ]]; then
+		print_to_console "Masking proc-sys-fs-binfmt_misc.automount"
+		systemctl mask proc-sys-fs-binfmt_misc.automount
+		systemctl daemon-reload
+	else 
+		print_to_console "proc-sys-fs-binfmt_misc.automount is already masked"
+	fi
+}
+
+function setup_modules () {
+	depmod -a 
+	modprobe irq_debug
+}
+
+# Retrieve DNS settings from DHCP leases as resolvconf is not setup
+# automatically from supplied settings
+# setup_dns
+
+# For some reason, depmod hasn't got around to worry about installed modules,
+# do it instead.
+# setup_modules
+
+# Link ONL Kernel headers to correct build directory.
+safe_link /usr/share/onl/packages/amd64/onl-kernel-${KERNEL_MAJOR_MINOR}-lts-x86-64-all/mbuilds/ /lib/modules/${KERNEL}/build
+
+# Link ONLP platform libraries. This will enable onlpd and onlp-snmpd to start.
+# rm -f /lib/x86_64-linux-gnu/libonlp-platform.so.1 # Is this needed?? If it's already a link, we re-link so not necessary
+# safe_link /lib/platform-config/${PLATFORM}-${REVISION}/onl/lib/libonlp-${PLATFORM}.so /lib/x86_64-linux-gnu/libonlp-platform.so.1
+# safe_link /lib/platform-config/${PLATFORM}-${REVISION}/ /lib/platform-config/current
+
+# Fixing shenanigans with ONLP -- unsure if needed.
+# safe_link /lib/platform-config/${PLATFORM}-${REVISION}/onl/bin/onlps /lib/platform-config/${PLATFORM}-${REVISION}/onl/bin/onlpdump
+
+# Disable automount.
+disable_automount
+

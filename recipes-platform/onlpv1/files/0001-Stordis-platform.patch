+    {                                      \
+        {                                  \
+            .id = ONLP_PSU_ID_CREATE(_id), \
+            .description = _desc,          \
+            .poid = 0,                     \
+        },                                 \
+        .status = 0,                       \
+        .caps = (ONLP_PSU_CAPS_DC12 | ONLP_PSU_CAPS_VOUT | ONLP_PSU_CAPS_POUT), \
+    }
+
+
+static onlp_psu_info_t onlp_psu_info[] = {
+    { }, /* Not used */
+    PSU_INFO_ENTRY_INIT(PSU_1, "PSU 1"),
+    PSU_INFO_ENTRY_INIT(PSU_2, "PSU 2"),
+};
